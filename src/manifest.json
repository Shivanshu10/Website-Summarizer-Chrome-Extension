{
    "name": "Website Summarizer",
    "description": "This extension will provide u with summary of selected text",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "19": "icon.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}